INICIO principal
    VAR n1: INTEIRO
    VAR n2: INTEIRO
    VAR resuldo: INTEIRO
    DEFINIR 0 -> n1
    DEFINIR 0 -> n2


    MOSTRAR "Digite o primeiro numero: "
    ESPERAR_DIGITACAO -> n1
    MOSTRAR "Digite o segundo numero: "
    ESPERAR_DIGITACAO -> n2
    SOMAR n1,n2 -> resuldo
    MOSTRAR resultado
FIM
